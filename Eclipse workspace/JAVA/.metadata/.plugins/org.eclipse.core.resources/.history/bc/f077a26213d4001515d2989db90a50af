<%
Connection con;
Class.forName("com.mysql.jdbc.Driver");
con=(Connection)DriverManager.getConnection("jdbc:mysql://localhost:3306/java", "java", "123456");
PreparedStatement ps=(PreparedStatement)con.prepareStatement("SELECT * from users");
ResultSet rs=ps.executeQuery();
while(rs.next()){
	String username=rs.getString("username");
	String password=rs.getString("password");
	out.println("Username "+username+" Pass "+password+"<hr>");
}
%>
