<%
Connection con;
Class.forName("com.mysql.jdbc.Driver");
con=(Connection)DriverManager.getConnection("jdbc:mysql://localhost:3306/Test", "root", "123");
PreparedStatement ps=(PreparedStatement)con.prepareStatement("SELECT * from test");
ResultSet rs=ps.executeQuery();
while(rs.next()){
	String sno=rs.getString("sno");
	String name=rs.getString("name");
	out.println("sno"+sno+" Name "+name+"<hr>");
}
%>
</body>
